{
  "expo": {
    "name": "Minesweeper",
    "slug": "Minesweeper",
    "version": "1.0.0",
    "orientation": "deafult",
    "icon": "./assets/appIcon.png",
    "androidNavigationBar":{
      "visible":"deafult"
    },
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F7F6E7"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png"
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
